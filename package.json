{
  "name": "svelte-ionic-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "npx eslint ./",
    "ionic:serve": "npm run dev --",
    "ionic:build": "npm run build --",
    "ionic-run-livereload:android": "ionic cap run android -l --external",
    "ionic-run-livereload:ios": "ionic cap run ios -l --external",
    "ionic-run:android": "ionic cap run android",
    "ionic-run:ios": "ionic cap run ios"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "autoprefixer": "^10.3.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-svelte3": "^3.2.1",
    "fibers": "^5.0.0",
    "postcss": "^8.3.6",
    "postcss-minify": "^1.1.0",
    "rollup": "^2.3.4",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-includepaths": "^0.2.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.39.0",
    "svelte": "^3.43.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "@capacitor/android": "^3.2.3",
    "@capacitor/cli": "^3.2.3",
    "@capacitor/core": "3.2.3",
    "@capacitor/ios": "3.2.3",
    "@ionic/core": "^5.8.1",
    "sirv-cli": "^1.0.0"
  }
}
